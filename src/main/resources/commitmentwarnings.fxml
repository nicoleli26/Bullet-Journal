<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="214.0" prefWidth="334.0">
      <children>
         <Label layoutY="6.0" text="Commitment Warnings" />
         <TextField fx:id="maxTasks" layoutX="99.0" layoutY="45.0" prefHeight="24.0" prefWidth="127.0" />
         <TextField fx:id="maxEvents" layoutX="99.0" layoutY="93.0" prefHeight="24.0" prefWidth="127.0" />
         <Label layoutX="63.0" layoutY="28.0" text="how many tasks would you like per day?" />
         <Label layoutX="78.0" layoutY="79.0" text="how many events would you like per day?" />
         <Button fx:id="saveCommits" layoutX="276.0" layoutY="187.0" mnemonicParsing="false" text="save" />
         <Button fx:id="closeCommits" layoutX="-1.0" layoutY="183.0" mnemonicParsing="false" text="done" />
      </children></AnchorPane>
</Scene>
